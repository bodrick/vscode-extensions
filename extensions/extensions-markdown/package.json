{
  "name": "vscode-extensions-markdown",
  "version": "0.1.0",
  "description": "Extensions useful for viewing and editing markdown files",
  "license": "MIT",
  "repository": {
    "directory": "extensions/extensions-markdown",
    "type": "git",
    "url": "https://github.com/bodrick/vscode-extensions"
  },
  "author": {
    "email": "bodrick@purplepig.net",
    "name": "bodrick"
  },
  "homepage": "https://github.com/bodrick/vscode-extensions",
  "bugs": {
    "email": "bodrick@purplepig.net",
    "url": "https://github.com/bodrick/vscode-extensions/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "scripts": {
    "build": "pwsh -noprofile -executionpolicy bypass -File build.ps1",
    "package": "vsce package",
    "publish": "pwsh -noprofile -executionpolicy bypass -File build.ps1 -Publish",
    "publish:patch": "pwsh -noprofile -executionpolicy bypass -File build.ps1 -Publish -Increment patch"
  },
  "categories": [
    "Extension Packs"
  ],
  "displayName": "Extensions useful for viewing and editing markdown files",
  "extensionPack": [
    "bierner.github-markdown-preview",
    "bierner.markdown-checkbox",
    "bierner.markdown-emoji",
    "bierner.markdown-footnotes",
    "bierner.markdown-mermaid",
    "bierner.markdown-preview-github-styles",
    "bierner.markdown-yaml-preamble",
    "DavidAnson.vscode-markdownlint",
    "shd101wyy.markdown-preview-enhanced",
    "yzhang.markdown-all-in-one"
  ],
  "icon": "icon-128x128.png",
  "publisher": "bodrick"
}
